<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Excel">
        <attr name="headerHeight" format="dimension"/>//标题高度
        <attr name="itemHeight" format="dimension"/>//每一行的高度
        <attr name="width" format="dimension"/>//表格总宽度
        <attr name="android:textSize"/>//表格的字体大小
        <attr name="choiceMode" format="enum">//选择模式
            <enum name="CHOICE_MODE_NONE" value="0"/>//不选
            <enum name="CHOICE_MODE_SINGLE" value="1"/>//单选
            <enum name="CHOICE_MODE_MULTIPLE" value="2"/>//多选
        </attr>
    </declare-styleable>
</resources>
