<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Defines the custom XML attributes supported for a HorizontalListView -->

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>


    <declare-styleable name="Excel">
        <attr name="filterHeight" format="dimension" />
        <attr name="headerHeight" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="width" format="dimension" />
        <attr name="hasFilter" format="boolean" />
        <attr name="freezeHeader" format="boolean" />
        <attr name="hasHeader" format="boolean" />
    </declare-styleable>
</resources>
